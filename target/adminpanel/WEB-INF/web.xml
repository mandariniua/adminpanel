<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddProductServlet</servlet-name>
  	<display-name>AddProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.plakhotnyuk.adminpanel.servlet.AddProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<display-name>DeleteProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.plakhotnyuk.adminpanel.servlet.DeleteProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AllProductServlet</servlet-name>
  	<display-name>AllProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.plakhotnyuk.adminpanel.servlet.AllProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>findProductByIDServlet</servlet-name>
  	<display-name>findProductByIDServlet</display-name>
  	<description></description>
  	<servlet-class>com.plakhotnyuk.adminpanel.servlet.findProductByIDServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddProductServlet</servlet-name>
  	<url-pattern>/AddProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<url-pattern>/DeleteProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AllProductServlet</servlet-name>
  	<url-pattern>/AllProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>findProductByIDServlet</servlet-name>
  	<url-pattern>/findProductByIDServlet</url-pattern>
  </servlet-mapping>
</web-app>
